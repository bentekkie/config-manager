{
  "name": "server-config-manager",
  "homepage": "https://github.com/bentekkie/config-manager",
  "repository": "github:bentekkie/config-manager",
  "version": "1.0.7",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bootstrap": "4.1.3",
    "codemirror": "^5.41.0",
    "cookie-parser": "~1.4.3",
    "express": "~4.16.0",
    "express-basic-auth": "^1.1.6",
    "http-errors": "^1.7.1",
    "ini": "^1.3.5"
  },
  "devDependencies": {
    "morgan": "~1.9.0",
    "react": "^16.6.1",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.6.1",
    "react-scripts": "2.1.1",
    "react-select": "^2.1.1",
    "react-split-pane": "^0.1.84",
    "react-ui-tree": "^4.0.0",
    "reactstrap": "^6.5.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "scripts": {
    "client:start": "cd client && react-scripts start",
    "client:build": "cd client && react-scripts build",
    "server:build": "npx babel server --out-dir server-build",
    "client:eject": "cd client && react-scripts eject",
    "start": "babel-node ./server/bin/www ~/.config/config-manager.ini",
    "prepublishOnly": "npm run client:build --scripts-prepend-node-path && npm run server:build --scripts-prepend-node-path"
  },
  "files": [
    "client/build/**/*",
    "server-build/**/*"
  ],
  "bin": "./server-build/bin/starter.js"
}
